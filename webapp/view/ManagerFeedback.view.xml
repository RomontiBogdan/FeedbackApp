<mvc:View
		controllerName="sap.ui.demo.walkthrough.controller.ManagerFeedback"
		xmlns:core="sap.ui.core"
	    xmlns:mvc="sap.ui.core.mvc"
        xmlns="sap.m"
		xmlns:f="sap.ui.layout.form"
        xmlns:l="sap.ui.layout">


			
    <Page
		    title="Manager Feedback"
		    showNavButton="true"
		    navButtonPress=".onNavBack"
	>

	 <Panel>
			<content>
						
						<!-- <Input
							id="Grade"
							placeholder="Give grade to employee"
							valueLiveUpdate="true"
							width="60%"
						/>
				 		<Button
           					 text="{Export to EXCEL and send}"
            				 press=".onLogIn"
            				 class="myCustomButton"
           					 icon="sap-icon://action"
          					 width="60%"
						/> -->

        <HBox  id = "Container1" headerText = "Details about User"  >
		<GenericTile 
            class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" 
			header="Fiscal Year"
			subheader="{FiscalYear}" 
			frameType="OneByOne">
			<TileContent>
				<ImageContent src="sap-icon://calendar" />
			</TileContent>
		</GenericTile>


		<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" 
			header="Employee name"
			subheader="{FullName}" 
			frameType="OneByOne">
			<TileContent>
				<ImageContent src="sap-icon://employee" />
			</TileContent>
		</GenericTile>

		<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" 
			header="Personnel Number"
			subheader="{PersonalNo}" 
			frameType="OneByOne">
			<TileContent>
				<ImageContent src="sap-icon://call" />
			</TileContent>
		</GenericTile>

		<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" 
			header="Current Career Level"
			subheader="{CareerLevel}" 
			frameType="OneByOne">
			<TileContent>
				<ImageContent src="sap-icon://leads" />
			</TileContent>
		</GenericTile>

		<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" 
			header="Organizational Assignment"
			subheader="{Su}" 
			frameType="OneByOne">
			<TileContent>
				<ImageContent src="sap-icon://crm-service-manager" />
			</TileContent>
		</GenericTile>
		 </HBox>



		<HBox id = "Container2">

		<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" 
			header="Project ID"
			subheader="{ProjectId}" 
			frameType="OneByOne"
			id = "GenericTileProjectId">
			<TileContent>
				<ImageContent src="sap-icon://group" />
			</TileContent>
		</GenericTile>

		<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" 
			header="Date of PEG"
			subheader="{SentAt}" 
			frameType="TwoByOne">
			<TileContent>
				<ImageContent src="sap-icon://date-time" />
			</TileContent>

		</GenericTile>

			<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" 
			header="Name of the Evaluator"
			subheader="{FromUser}" 
			frameType="OneByOne">
			<TileContent>
				<ImageContent src="sap-icon://activity-individual" />
			</TileContent>
			
		</GenericTile>


		<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" 
			header="Number of Project Days Evaluated"
			subheader="{DaysEvaluated}" 
			frameType="OneByOne">
			<TileContent>
				<ImageContent src="sap-icon://timesheet" />
			</TileContent>
			
		</GenericTile>

		</HBox>

		<HBox id = "Container3">


		<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" 
			header="Name of the project"
			subheader="{ProjectName}" 
			frameType="TwoByOne">
			<TileContent>
				<ImageContent src="sap-icon://idea-wall" />
			</TileContent>
		
		</GenericTile>

		<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" 
			header="Name of the Project Manager"
			subheader="{ProjectManager}" 
			frameType="OneByOne">
				<TileContent>
				<ImageContent src="sap-icon://manager" />
			</TileContent>
			
		</GenericTile>

		<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" 
			header="Customer name"
			subheader="{Customer}" 
			frameType="OneByOne">
			<TileContent>
				<ImageContent src="sap-icon://customer" />
			</TileContent>
			
		</GenericTile>

	</HBox>
			</content>
			</Panel>


		<Table id="idcoredata" inset="false" items="{/coredata}">
	<headerToolbar>
	</headerToolbar>
	<columns>
		<Column width="12em" hAlign="Center">
			<Text text="Criteria"/>
		</Column>
		<Column width="12em" hAlign="Center">
			<Text text="Rating"/>
		</Column>
		<Column width="12em" hAlign="Center">
			<Text text="Description of the rating"/>
		</Column>
		<Column width="12em" hAlign="Center">
			<Text text="Recommendations / Comments"/>
		</Column>
		
	</columns>
	<items>
		<ColumnListItem>
			<cells>
				


			</cells>
		</ColumnListItem>
	</items>
</Table>

		<footer>
			<Toolbar>
				<ToolbarSpacer/>
				<Button
					text="Export to excel"
					class="myCustomButton"
					press="onExport"
					icon="sap-icon://download"
				/>
			</Toolbar>
		</footer>
    </Page>

</mvc:View>