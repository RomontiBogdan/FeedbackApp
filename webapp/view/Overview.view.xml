<mvc:View
	controllerName="sap.ui.demo.walkthrough.controller.App"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
>
	<Page
		enableScrolling="false"
		class="backgroundGradient"
	>
		<customHeader>
			<Bar>
				<contentRight>
					<Button
						icon="../resources/UK_Flag.png"
						press=".setEnglishi18n"
						class="sapUiSmallMarginBeginEnd"
					/>
					<Button
						icon="../resources/DE_Flag.png"
						press=".setGermani18n"
						class="sapUiTinyMarginEnd"
					/>
				</contentRight>
				<contentLeft>
					<Image
						src="../resources/MHP_Logo_RGB.svg"
						height="75%"
					/>
				</contentLeft>
			</Bar>
		</customHeader>
		<content>
			<mvc:XMLView viewName="sap.ui.demo.walkthrough.view.LogReg"/>
		</content>
	</Page>
</mvc:View>