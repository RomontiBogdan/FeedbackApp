<mvc:View
	controllerName="sap.ui.demo.walkthrough.controller.Main"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
>
	<Page title="{i18n>MainPageTitle}">
		<headerContent>
			<Button 
			text="{i18n>ProfileBtn}"
			icon="sap-icon://account"
			press=".onProfile"/>
			<Button 
			tooltip="Log Out"
			type="Emphasized"
			icon="sap-icon://visits"
			press=".onLogOut"/>
		</headerContent>
		<FlexBox
				fitContainer="true"
				alignItems="Center"
				justifyContent="Center">
				<GenericTile
					class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
					header="{i18n>PEG}"
					mode="HeaderMode"
					backgroundImage="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAOEAAADhCAMAAAAJbSJIAAAAA1BMVEXP3/n/SH/PAAAASElEQVR4nO3BgQAAAADDoPlTX+AIVQEAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAADwDcaiAAFXD1ujAAAAAElFTkSuQmCC"
					frameType="TwoByOne"
					press=".onPEG"
				/>
				<GenericTile
					class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
					header="{i18n>Feedback}"
					mode="HeaderMode"
					backgroundImage="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAOEAAADhCAMAAAAJbSJIAAAAA1BMVEXP3/n/SH/PAAAASElEQVR4nO3BgQAAAADDoPlTX+AIVQEAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAADwDcaiAAFXD1ujAAAAAElFTkSuQmCC"
					frameType="TwoByOne"
					press=".onFeedback"
				/>
				<GenericTile
					class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
					header="{i18n>TeamFeedback}"
					mode="HeaderMode"
					backgroundImage="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAOEAAADhCAMAAAAJbSJIAAAAA1BMVEXP3/n/SH/PAAAASElEQVR4nO3BgQAAAADDoPlTX+AIVQEAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAADwDcaiAAFXD1ujAAAAAElFTkSuQmCC"
					frameType="TwoByOne"
					press=".onTeamFeedback"
				/>
		</FlexBox>
