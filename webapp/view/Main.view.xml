<mvc:View

	controllerName="sap.ui.demo.walkthrough.controller.Main"
	xmlns="sap.m"
	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
	xmlns:mvc="sap.ui.core.mvc">
	
		<Page title="User"
        showNavButton="true"
		navButtonPress=".onNavBack">

 	<FlexBox alignItems="Center" justifyContent="Center">
	  	<Panel>
			 <content>
 
            <Button
            	text="{i18n>RequestPEG}"
           		class="myCustomButton"
				press=".onRequestNewPEG"
            	width="60%"/>

		
			<Button
            	text="{i18n>DisplayPEGList}"
           		class="myCustomButton"
				press=".onDisplayPEGList"
            	width="60%"/>
	   

			<Button
            	text="{i18n>ListSentReceived}"
           		class="myCustomButton"
				press = ".onListSentReceived"
            	width="60%"/>

			<Button
            	text="{i18n>ViewProfile}"
           		class="myCustomButton"
				press = ".onProfile"
            	width="60%"/>


			<Button
            	text="{i18n>NewFeedback}"
           		class="myCustomButton"
            	width="60%"/>

			<Button
            	text="{i18n>LogOut}"
				press=".onLogIn"
           		class="myCustomButton"
            	width="60%"/>
				
       		</content>
	  	 </Panel>
	   </FlexBox>
	  
    </Page>

</mvc:View>